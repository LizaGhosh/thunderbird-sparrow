# Voice Note Parser Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Claude AI (Anthropic) - Required for Claude models
CLAUDE_API_KEY=your_claude_api_key_here

# Gemini AI (Google) - Required for Gemini models  
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Set default AI provider (claude or gemini)
# DEFAULT_AI_PROVIDER=claude

# Optional: Set default model
# DEFAULT_MODEL=claude-3-opus-20240229

# Optional: Set default temperature (0.0 to 1.0)
# DEFAULT_TEMPERATURE=0.0
